package shared

templ Page( title string ) {
   <!DOCTYPE html>
   <html lang="en">
   <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <script src="https://unpkg.com/htmx.org@1.5.0/dist/htmx.js"></script>
    <title>{ title }</title>
   </head>
   <body>
    @Navbar()
    <h1>{ title }</h1>
   </body>
   </html> 
}
